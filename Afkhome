              }}
            >
              {/* <div className='row'>


          Do you want to report this comment?
                  </div> */}
              <img
                src={MIe01}
                className="mis-icon ebtdialogicon"
                alt="edit-icon"
                width="48"
                height="48"
              />
              <DialogFooter>
                <DefaultButton className='' onClick={() => this.closestage1CommentReportDialog()} text={langText.closed} />
